    "@angular/http": "^4.3.4",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@angular/platform-server": "^4.0.1",
    "@angularclass/hmr-loader": "^3.0.4",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.3",
    "bluebird": "^3.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.0",
    "css-loader": "^0.28.4",
    "es6-shim": "^0.35.1",
    "es6-symbol": "^3.0.2",
    "es7-reflect-metadata": "^1.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.5.2",
    "rollup": "^0.47.4",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.18.2",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "ts-node": "~3.0.4",
    "tslint": "^3.10.2",
    "tslint-loader": "^2.1.4",
    "typescript": "~2.2.0",
    "url-loader": "^0.5.9",
    "web-animations-js": "^2.3.1",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "ngc": "ngc --p config/tsconfig.dist.json",
    "postngc": "node node_modules/rimraf/bin src/**/app.ngfactory.* src/**/*.ngsummary.json",
    "prebundle": "node  node_modules/rollup/bin/rollup -c config/rollup.js",
    "bundle": "uglifyjs --compress --mangle --output bundles/core.umd.min.js -- bundles/core.umd.js",
    "clean": "node node_modules/rimraf/bin dist bundles",
    "prebuild": "npm run clean",
    "build": "npm run ngc && npm run bundle",
    "serve": "node node_modules/webpack-dev-server/bin/webpack-dev-server --port=8011 --hot --progress --config config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbmh/ngtable.git"
  },
  "keywords": [
    "typescript",
    "angular2",
    "ng2",
    "table",
    "pagination"
  ],
  "author": "Michał Hałas <http://noisyboy.pl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbmh/ngtable/issues"
  },
  "homepage": "https://github.com/nbmh/ngtable#readme"
}
