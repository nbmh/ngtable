{
  "name": "ngtable",
  "version": "0.5.1",
  "description": "angular2 table/pagination support",
  "main": "bundles/core.umd.js",
  "module": "dist/app.js",
  "typings": "dist/app.d.ts",
  "dependencies": {
    "@angular/core": "^4.3.4",
    "@angular/common": "^4.3.4",
    "@angular/router": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "hammerjs": "^2.0.8",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/animations": "^4.3.4",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/compiler": "^4.0.1",
